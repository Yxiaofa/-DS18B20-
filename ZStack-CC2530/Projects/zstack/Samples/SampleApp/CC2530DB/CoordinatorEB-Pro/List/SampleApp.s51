//  186   halUARTCfg_t uartConfig;//顶一个串口结构体
//  187   uartConfig.configured             =TRUE;//串口配置为真
        MOV     DPL,?XSP + 0
        MOV     DPH,?XSP + 1
        INC     A
        MOVX    @DPTR,A
//  188   uartConfig.baudRate               =HAL_UART_BR_9600;//波特率为9600
        LCALL   ?XSTACK_DISP0_8
